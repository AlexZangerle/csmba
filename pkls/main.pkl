amends "https://raw.githubusercontent.com/UIBK-DPS-DC/Cirrina-Specifications/main/pkl/CollaborativeStateMachineDescription.pkl"
import "https://raw.githubusercontent.com/UIBK-DPS-DC/Cirrina-Specifications/main/pkl/CollaborativeStateMachineDescription.pkl" as CSM
import "https://raw.githubusercontent.com/AlexZangerle/csmba/main/pkls/events.pkl" as Events
import "https://raw.githubusercontent.com/AlexZangerle/csmba/main/pkls/serviceTypes.pkl" as ServiceTypes
import "https://raw.githubusercontent.com/AlexZangerle/csmba/main/pkls/variables.pkl" as Vars
import "https://raw.githubusercontent.com/AlexZangerle/csmba/main/pkls/eletricalSafety/eletricalSafety.pkl" as eletricalSafety
import "https://raw.githubusercontent.com/AlexZangerle/csmba/main/pkls/gasSafety/gasSafety.pkl" as gasSafety
import "https://raw.githubusercontent.com/AlexZangerle/csmba/main/pkls/tempSafety/tempSafety.pkl" as tempSafety
import "https://raw.githubusercontent.com/AlexZangerle/csmba/main/pkls/shading/shading.pkl" as shading
import "https://raw.githubusercontent.com/AlexZangerle/csmba/main/pkls/lighting/lighting.pkl" as lighting
import "https://raw.githubusercontent.com/AlexZangerle/csmba/main/pkls/roomOccupancy/roomPccupancy.pkl" as roomOccupancy
import "https://raw.githubusercontent.com/AlexZangerle/csmba/main/pkls/fire/fire.pkl" as fire

name = "Building System"
version = "2.0"

stateMachines {
    lighting.lightingSM
    shading.shadingSM
    roomOccupancy.roomOccupancySM
    fire.fireDetectionSM
    tempSafety.temperatureSafetySM
    eletricalSafety.electricalSafetySM
    gasSafety.gasSafetySM
}
persistentContext {
  variables {
    new {
      name = Vars.vTemperature
      value = "'0'"
    }
    new {
        name = Vars.vEnergySaving
        value = "false"
    }
    new {
        name = Vars.vCurrentOccupancy
        value = "'vacant'"
    }
    new {
        name = Vars.vFireAlarm
        value = "false"
    }
    new {
        name = Vars.vSmokeAlert
        value = "false"
    }
    new {
        name = Vars.vTempThreshold
        value = "'60'"
    }
  }
}